              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ghGxRIs1EeCHZZoM5jy0Lg" name="initEngine" specification="_geW_Q4s1EeCHZZoM5jy0Lg">
                <language>java</language>
                <body>if (null == mEngine) {
			EngineConfig config = initConfig();

			mEngine = EREngine.getInstance();
			mEngine.setApplication(this.getApplication());
			mEngine.setContext(this);
			mEngine.setConfig(config);
			mEngine.setRender(new Render2(mPageView));
			mEngine.setRecorder(new FileRecorder(this));
			mEngine.setUpdateViewEnabled(true);
			setErrorHandler();
		}
	</body>
