              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ghHYV4s1EeCHZZoM5jy0Lg" name="setErrorHandler" specification="_geYNY4s1EeCHZZoM5jy0Lg">
                <language>java</language>
                <body>// 輸入密碼的回調
		IErrorCall inputPdfPassword = new IErrorCall() {

			@Override
			public void handleError() {
				showPdfInputDialog();
			}
		};

		IErrorCall inputEpubPassword = new IErrorCall() {

			@Override
			public void handleError() {
				showEpubInputDialog();
			}
		};

		// 提示書籍不存在的回調
		IErrorCall bookNotExists = new IErrorCall() {
			@Override
			public void handleError() {
				// 退出
				finish();
			}
		};

		IErrorCall hyfLoadPageFinished = new IErrorCall() {
			@Override
			public void handleError() {
				iaUpdatePage.action();
			}
		};

		IErrorCall commentNotInit = new IErrorCall() {
			@Override
			public void handleError() {
				Logger.eLog(TAG, &quot;comment not init&quot;);
			}
		};

		IErrorCall commentSaveError = new IErrorCall() {
			@Override
			public void handleError() {
				Logger.eLog(TAG, &quot;comment save error&quot;);
			}
		};

		IErrorCall commentDelError = new IErrorCall() {
			@Override
			public void handleError() {
				Logger.eLog(TAG, &quot;comment delete error&quot;);
			}
		};

		IErrorCall maxFontError = new IErrorCall() {
			@Override
			public void handleError() {
				Logger.eLog(TAG, &quot;字體已經最大&quot;);
			}
		};

		IErrorCall minFontError = new IErrorCall() {
			@Override
			public void handleError() {
				Logger.eLog(TAG, &quot;字體已經最小&quot;);
			}
		};

		// 針對不同的錯誤碼設置響應的錯誤處理
		if (mEngine != null) {
			mEngine.setErrorHandler(EngineMsgCode.ENGINE_ERROR_BOOK_NOT_EXISTS,
					bookNotExists);
			mEngine.setErrorHandler(
					AdobeMsgCode.STATE_ERROR_PDF_T3_NEED_PASSWORD,
					inputPdfPassword);
			mEngine.setErrorHandler(
					AdobeMsgCode.STATE_ERROR_PASSHASH_NOT_FOUND,
					inputEpubPassword);
			mEngine.setErrorHandler(
					EngineMsgCode.ENGINE_ERROR_COMMENT_NOT_INIT, commentNotInit);
			mEngine.setErrorHandler(
					EngineMsgCode.ENGINE_ERROR_COMMENT_SAVE_FAILED,
					commentSaveError);
			mEngine.setErrorHandler(
					EngineMsgCode.ENGINE_ERROR_COMMENT_DELETE_FAILED,
					commentDelError);
			mEngine.setErrorHandler(EngineMsgCode.ENGINE_WARN_MAX_FONT,
					maxFontError);
			mEngine.setErrorHandler(EngineMsgCode.ENGINE_WARN_MIN_FONT,
					minFontError);
			// mEngine.setErrorHandler(
			// HyfMsgCode.STATE_NORMAL_LOAD_BOOK_FINISHED,
			// hyfLoadPageFinished);
		}
	</body>
              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ghH_YIs1EeCHZZoM5jy0Lg" name="onKeyDown" specification="_ggIg4Is1EeCHZZoM5jy0Lg">
                <ownedParameter xmi:id="_ghH_YYs1EeCHZZoM5jy0Lg" name="keyCode">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                </ownedParameter>
                <ownedParameter xmi:id="_ghH_Yos1EeCHZZoM5jy0Lg" name="event" type="_ggHSwYs1EeCHZZoM5jy0Lg"/>
                <ownedParameter xmi:id="_ghH_Y4s1EeCHZZoM5jy0Lg" direction="return">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                </ownedParameter>
                <language>java</language>
                <body>Logger.dLog(TAG, &quot;onKeyDown keyCode = &quot; + keyCode);
		
		switch (keyCode) {
		// ek1 code
		case 93:
			pageDown();
			break;
		case 92:
			pageUp();
			break;
		case 95:
			zoomOut();
			return true;
		case 94:
			zoomIn();
			return true;

			// em1 code
			// case KeyEvent.KEYCODE_DPAD_DOWN:
			// pageDown();
			// break;
			// case KeyEvent.KEYCODE_DPAD_UP:
			// pageUp();
			// break;
			// case KeyEvent.KEYCODE_VOLUME_DOWN:
			// zoomOut();
			// return true;
			// case KeyEvent.KEYCODE_VOLUME_UP:
			// zoomIn();
			// return true;

		case 82:
			showControlPanelDialog();
			break;
		default:
			break;
		}
		return super.onKeyDown(keyCode, event);
	</body>
              </ownedBehavior>
